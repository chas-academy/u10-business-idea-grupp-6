{
    "name": "gamerhub/gamer-hub",
    "description": "A Laravel Nova theme.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Gamerhub\\GamerHub\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gamerhub\\GamerHub\\ThemeServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
